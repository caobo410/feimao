<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!--品牌列表-->
        <record id="view_tree_fm_agent" model="ir.ui.view">
            <field name="name">代理商</field>
            <field name="model">fm.agent</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <tree string="view">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="persons_responsible"/>
                    <field name="tel"/>
                    <field name="operator"/>
                </tree>
            </field>
        </record>

        <!--门店列表-->
        <record id="view_tree_agent_line" model="ir.ui.view">
            <field name="name">联系人</field>
            <field name="model">agent.line</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <tree string="view">
                    <field name="name"/>
                    <field name="persons_responsible"/>
                    <field name="tel"/>
                    <field name="address"/>
                </tree>
            </field>
        </record>

    </data>
</openerp>
