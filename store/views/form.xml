<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- 门店列表 -->
        <record id="view_form_fm_store" model="ir.ui.view">
            <field name="name">门店管理</field>
            <field name="model">fm.store</field>
            <field name="arch" type="xml">
                <form string="窗体名称" version="7.0">
                    <sheet>
                        <h1>
                            编号<field name="code" class="oe_inline"/>
                        </h1>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="address"/>
                                <field name="open_date"/>
                                <field name="messages"/>
                            </group>
                            <group>
                                <field name="persons_responsible"/>
                                <field name="operator"/>
                                <field name="line_id"/>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="user_id"/>
                            </group>
                            <group>
                                <field name="date_confirm"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</openerp>
